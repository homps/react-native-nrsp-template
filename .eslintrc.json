{
    "parser": "babel-eslint",
    "env": {
        "react-native/react-native": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react-native/no-unused-styles": 2,
        "react/prop-types": ["error", { "ignore": ["navigation", "navigationOptions", "screenProps"] }]
    },
    "ecmaFeatures": {
        "jsx": true
      }
}